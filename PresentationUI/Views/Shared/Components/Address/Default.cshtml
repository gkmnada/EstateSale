@model List<ResultAddressDto>

<div class="col-lg-4 cont-details">
    @foreach (var item in Model)
    {
        <address>
            <h5 class="">Ofis Adresimiz</h5>
            <p>
                <span class="fa fa-map-marker"></span>@item.address_detail
            </p>
            <h5 class="mt-4 pt-lg-3">İletişim Bilgilerimiz</h5>
            <p>
                <span class="fa fa-phone"></span> <strong>Telefon :</strong>
                <a href="tel:@item.phone"> @item.phone</a>
            </p>
            <p>
                <span class="fa fa-envelope"></span> <strong>Mail Adresi :</strong>
                <a href="mailto:@item.email"> @item.email</a>
            </p>
        </address>
    }
</div>